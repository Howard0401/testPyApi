<?xml encoding="utf-8" version="1.0"?>
<configuration>
    <appSettings>
        <add key="WSGI_HANDLER" value="<app.py>.app"/>
        <add key="PYTHONPATH" value="D:\home\site\wwwroot"/>
        <add key="WSGI_LOG" value="D:\home\LogFiles\wfastcgi.log"/>
    </appSettings>
    <system.webServer>
        <handlers>
            <add modules="FastCgiModule" name="PythonHandler" path="*" requireAccess="Script" resourceType="Unspecified" scriptProcessor="D:\home\Python361x64\python.exe|D:\home\Python361x64\wfastcgi.py" verb="*"/>
        </handlers>
    </system.webServer>
</configuration>
